@model IEnumerable<asociatie_proprietari.Models.Apartament>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NumarApartament)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Scara)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumarCamere)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumarPersoane)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumarApartament)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Scara)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumarCamere)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumarPersoane)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ApartamentId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ApartamentId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ApartamentId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
